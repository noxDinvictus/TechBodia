{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prettier": "prettier --write \"**/*.+(ts|tsx|js|json|scss|md|vue)\"",
    "code:checker": "yarn build && yarn generate",
    "deploy": "nuxt generate && gh-pages -d .output/public",
    "dto:download": "curl https://localhost:44365/swagger/v1/swagger.yaml -o C:\\temp\\techbodia.yaml --insecure -s",
    "dto:generate": "yarn swagger-typescript-api --path C:\\temp\\techbodia.yaml --output ./data --name dtos.ts --no-client --silent",
    "generate:model": "node generateModel.js",
    "dto": "yarn dto:download && yarn dto:generate && yarn generate:model",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.+(ts|vue|js|md)": [
      "yarn prettier",
      "yarn code:checker"
    ]
  },
  "dependencies": {
    "@nuxt/image": "1.9.0",
    "@pinia/nuxt": "^0.9.0",
    "@types/cookie": "^1.0.0",
    "axios": "^1.7.9",
    "gh-pages": "^6.3.0",
    "lint-staged": "^15.4.3",
    "moment": "^2.30.1",
    "nuxt": "^3.15.4",
    "nuxt-storage": "^1.2.2",
    "pinia": "^2.3.1",
    "prettier": "^3.5.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "^1.84.0",
    "swagger-typescript-api": "^13.0.23",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^3.4.12",
    "toastify-js": "^1.12.0",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@types/toastify-js": "^1.12.3",
    "husky": "^8.0.0"
  }
}
